sm <- merge(index, sm, by = c("i", "j"), all.x = TRUE, sort=FALSE)
sm <- sm[sm$x != 0 & !is.na(sm$x),]
attr(sm$i, "label") <- r
attr(sm$j, "label") <- c
class(sm) <- c("sparse.matrix", class(sm))
