t.sparse.matrix <- function(a) {
# swap i and j index
ta <- a
names(ta) <- c('j', 'i', 'x')
ta <- ta[,c('i', 'j', 'x')]
r <- attr(ta$i, "label"); c <- attr(ta$j, "label")
index <- data.frame(i=rep(1:r), j=rep(1:c, each=r))
ta <- merge(index, ta, by = c("i", "j"), all.x = TRUE, sort=FALSE)
ta <- ta[ta$x != 0 & !is.na(ta$x),]
