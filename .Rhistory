runsOf3s <- function(x){
r <- rep(0, length(x))
if(x[1] == 3)r[1] <- 1
for(i in 2:length(x)){
if(x[i] == 3){r[i] <- r[i-1] + 1}else{r[i] <- 0}
}
return(r)
}
maxRunOf3s <- function(x){
return(max(runsOf3s(x)))
}
